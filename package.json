{
    "name": "app",
    "version": "1.0.0",
    "description": "",
    "main": "main.js",
    "scripts": {
        "clean": "rimraf ../webpack_app ./artifacts ./build/reports/",
        "start:dev": "webpack-dev-server --env.dev --env.client_name=ctdot --progress",
        "start:prod": "webpack --config ./webpack.config.js --env.prod --env.client_name=ctdep --progress",
        "test": "jest",
        "lint": "eslint src",
        "format": "npm run prettier -- --write",
        "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
        "validate": "npm run lint && npm run test && npm run prettier -- --list-different",
        "precommit": "lint-staged",
        "create-bundle-analysis-log": "webpack -p --env.prod --env.client_name=ctdot --json --progress --profile > webpack-build-log.json",
        "show-dependency-graph": "webpack-bundle-analyzer webpack-build-log.json",
        "analyze:bundles": "npm run create-bundle-analysis-log && npm run show-dependency-graph",
        "test:watch": "jest --watch",
        "test:cypress": "node_modules/.bin/cypress open",
        "test:cypress-dashboard": "node_modules/.bin/cypress run --record",
        "doc:esdoc": "esdoc -c ./config/esdoc.json",
        "doc:md": "jsdoc2md --configure ./config/jsdoc.json \"src\\**\\*.js\" > ./artifacts/doc/api.md",
        "doc": "npm run doc:esdoc && npm run doc:md",
        "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
        "dev": "npm run webpack-dev-server -- --mode development",
        "dev:watch": "npm run webpack -- --mode development",
        "prod": "npm run webpack -- --mode production",
        "dev:debug": "npm run debug -- --mode development",
        "prod:debug": "npm run debug -- --mode production",
        "debugthis": "node --inspect --inspect-brk ./src/index.js"
    },
    "private": true,
